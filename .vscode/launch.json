    "version": "0.2.0",
    "configurations": [
        {
            "name": "[1 GPUs] LightGCN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole_gnn.py",
            "args": [
                "--model",
                "LightGCN",
                "--dataset",
                "test",
                "--config_files",
                "./recbole_gnn/properties/overall.dev.yaml",
                "--nproc",
                "1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "[n GPUs] GNN Group",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole_gnn_group.py",
            "args": [
                "-m",
                "LESSR,NISER",
                "--dataset",
                "test",
                "--config_files",
                "./recbole_gnn/properties/overall.dev.yaml",
                "--nproc",
                "-1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "[1 GPUs] GNN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole_gnn.py",
            "args": [
                "--model",
                "GRU4Rec",
                "--dataset",
                "ml-100k",
                "--config_files",
                "./recbole_gnn/properties/overall.dev.yaml",
                "--nproc",
                "1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "[n GPUs] GNN",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_recbole_gnn.py",
            "args": [
                "--model",
                "GRU4Rec",
                "--dataset",
                "ml-100k",
                "--config_files",
                "./recbole_gnn/properties/overall.dev.yaml",
                "--nproc",
                "-1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}